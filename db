case $1 in
 jindi_old)
        mysql -hrr-2ze61dsj9d3xl34s3425.mysql.rds.aliyuncs.com -ujindi -pjindichangsheng prism1
        ;;
 jindi_new)
        mysql -hrm-2ze99co2cx336f809.mysql.rds.aliyuncs.com -uapp_zhengxin -pTY3Pbc8Y%nq2#sld prism1
        ;;
 jindi_test)
        mysql -hrm-2zel20s95p9qwv076.mysql.rds.aliyuncs.com -ujindi -pJ1ndiSpidertest
        ;;
 local)
        mysql -ubamboo -pbamboo
        ;;
 darwin)
        mysql -hdarwin -ubamboo -pbamboo
        ;;
 *)
        echo "Usage: $schema [jindi_old|jindi_new|jindi_test|local|darwin]"
        exit 1
        ;;
esac
